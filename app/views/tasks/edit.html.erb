<h1>Tasks#edit</h1>
<%= form_with model: @task, url: {action: :update} , local: true do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :memo %>
<%= f.text_field :memo  %>
<%= select :task, :category_id, @categories.map{ |c| [c.title, c.id] }, {}, { class: 'form-control' } %>
<%= f.submit "登録する" %>
<% end %>
