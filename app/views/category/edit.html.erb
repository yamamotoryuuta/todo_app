<div class="task_container">
  <h1>カテゴリーを編集する</h1>
  <%= form_with model: @category, url:{ action: :update} do |f| %>
  <div class="form_card">
      <%= f.label :title, class: 'form_label'%>
      <%= f.text_field :title, class: 'form_field' %>
  </div>
  <%= f.submit "編集する", data: { confirm: "編集してもいいですか？" } %>
  <% end %>

  <p style="margin-top:20px;"><%= link_to "topへ戻る", root_path %></p>
  
</div>